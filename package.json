		"@storybook/react": "^3.2.16",
		"@types/react": "^16.0.25",
		"@types/react-dom": "^16.0.3",
		"@types/react-router-dom": "^4.2.3",
		"body-parser": "^1.18.2",
		"chalk": "^2.3.0",
		"downshift": "^1.22.1",
		"ejs": "^2.5.7",
		"enzyme": "^3.2.0",
		"express": "^4.16.2",
		"jest": "^21.2.1",
		"jest-enzyme": "^4.0.1",
		"prettier": "^1.8.2",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-router-dom": "^4.2.2",
		"styled-components": "^2.2.4",
		"ts-loader": "^3.2.0",
		"typescript": "^2.6.2",
		"webpack": "^3.9.1"
	}
